{
  "name": "video-manager-ui",
  "version": "1.0.0",
  "description": "",
  "author": "Giovanni Tommasi",
  "scripts": {
    "relay": "relay-compiler",
    "start": "react-dotenv && yarn relay && react-scripts start",
    "build": "react-dotenv && yarn relay && react-scripts build",
    "lint": "npx eslint '**/*.ts' '**/*.tsx' --fix --ignore-pattern node_modules/",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test-components": "test-storybook --watch",
    "test-e2e": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --browsers chromium firefox webkit"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.gql",
    "language": "typescript",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-relay": "^14.1.2",
    "classnames": "^2.3.2",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "rdndmb-html5-to-touch": "^8.0.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.0",
    "react-dnd-preview": "^8.0.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-relay": "^15.0.0",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "^15.0.0",
    "typescript": "^5.0.4",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.4",
    "@mdx-js/react": "^2.1.5",
    "@playwright/test": "^1.32.1",
    "@storybook/addon-actions": "^7.0.7",
    "@storybook/addon-docs": "^7.0.7",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addons": "^7.0.7",
    "@storybook/jest": "^0.1.0",
    "@storybook/node-logger": "^7.0.7",
    "@storybook/preset-create-react-app": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-relay": "^15.0.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "^29.3.1",
    "path": "npm:path-browserify@^1.0.1",
    "prop-types": "^15.8.1",
    "react-dotenv": "^0.1.3",
    "relay-compiler": "^15.0.0",
    "storybook": "^7.0.7",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "webpack": "^5.75.0"
  },
  "react-dotenv": {
    "whitelist": [
      "SERVER_URL"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.3.0"
}
